.myReservation .list {
  padding: 0 25rpx 20rpx;
}
.myReservation .list .item {
  margin-top: 20rpx;
  background: #ffffff;
  border: 1rpx solid #b5b5b5;
}
.myReservation .list .item .title {
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 70rpx;
  font-size: 24rpx;
  padding: 0 20rpx;
  border-bottom: 1rpx solid #b5b5b5;
}
.myReservation .list .item .title .no {
  color: #333333;
}
.myReservation .list .item .title .time {
  color: #999999;
}
.myReservation .list .item .infor {
  padding: 12rpx 20rpx 18rpx;
}
.myReservation .list .item .infor .p {
  display: flex;
  font-size: 24rpx;
  color: #03447c;
}
.myReservation .list .item .infor .p .val {
  flex: 1;
  overflow: hidden;
}
.myReservation .list .item .infor .obj {
  height: 50rpx;
  justify-content: space-between;
  align-items: center;
}
.myReservation .list .item .infor .obj .tit {
  display: flex;
}
.myReservation .list .item .infor .obj .btn {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 120rpx;
  height: 44rpx;
  color: #ffffff;
  border-radius: 22rpx;
  background: #dddddd;
}
.myReservation .list .item .infor .obj .active {
  background: #ff592c;
}
.myReservation .list .item .infor .time {
  align-items: center;
  height: 50rpx;
}
.myReservation .list .item .infor .addr {
  min-height: 50rpx;
  margin-top: 10rpx;
}

